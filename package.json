{
  "name": "@effective/css",
  "version": "0.1.0",
  "description": "A modern, layered CSS reset for evergreen browsers using @layer",
  "keywords": [
    "css",
    "reset",
    "normalize",
    "css-reset",
    "css-layer",
    "cascade-layers",
    "modern-css"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/effective-css-reset.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/effective-css-reset/issues"
  },
  "homepage": "https://github.com/your-username/effective-css-reset#readme",
  "type": "module",
  "main": "src/index.css",
  "style": "src/index.css",
  "exports": {
    ".": "./src/index.css",
    "./reset": "./src/reset.css",
    "./reset.css": "./src/reset.css",
    "./fixes": "./src/fixes.css",
    "./fixes.css": "./src/fixes.css",
    "./elements": "./src/elements.css",
    "./elements.css": "./src/elements.css"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "lint": "stylelint \"src/**/*.css\"",
    "lint:fix": "stylelint \"src/**/*.css\" --fix",
    "check": "node scripts/check-syntax.js",
    "serve": "npx serve docs -p 3000"
  },
  "devDependencies": {
    "stylelint": "^16.11.0",
    "stylelint-config-standard": "^36.0.1"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "engines": {
    "node": ">=18"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 Edge versions"
  ]
}
